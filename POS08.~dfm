  object QTicket: TADOQuery
    Connection = DM.ADOSIGMA
    CursorType = ctStatic
    LockType = ltBatchOptimistic
    OnCalcFields = QTicketCalcFields
    Parameters = <
      item
        Name = 'usu'
        Attributes = [paSigned]
        DataType = ftInteger
        Precision = 10
        Size = 4
        Value = Null
      end
      item
        Name = 'fec1'
        Attributes = [paNullable]
        DataType = ftDateTime
        NumericScale = 3
        Precision = 23
        Size = 16
        Value = Null
      end
      item
        Name = 'fec2'
        Attributes = [paNullable]
        DataType = ftDateTime
        NumericScale = 3
        Precision = 23
        Size = 16
        Value = Null
      end
      item
        Name = 'caj'
        Attributes = [paSigned]
        DataType = ftInteger
        Precision = 10
        Size = 4
        Value = Null
      end>
    SQL.Strings = (
      'select'
      'usu_codigo, fecha, caja, ticket,'
      'total, descuento, NCF_Fijo, NCF_Secuencia,'
      'itbis, nombre, rnc, status'
      'from'
      'montos_ticket'
      'where'
      'usu_codigo = :usu'
      
        'and fecha_hora between convert(datetime, :fec1, 113) and convert' +
        '(datetime, :fec2, 113)'
      'and caja   = :caj'
      'and status not in ('#39'CAN'#39','#39'ANU'#39','#39'ABI'#39')')
    Left = 160
    Top = 200
    object QTicketusu_codigo: TIntegerField
      FieldName = 'usu_codigo'
    end
    object QTicketfecha: TDateTimeField
      FieldName = 'fecha'
    end
    object QTicketcaja: TIntegerField
      FieldName = 'caja'
    end
    object QTicketticket: TIntegerField
      FieldName = 'ticket'
    end
    object QTickettotal: TBCDField
      FieldName = 'total'
      currency = True
      Precision = 15
      Size = 2
    end
    object QTicketdescuento: TBCDField
      FieldName = 'descuento'
      currency = True
      Precision = 15
      Size = 2
    end
    object QTicketNCF_Fijo: TStringField
      FieldName = 'NCF_Fijo'
      Size = 11
    end
    object QTicketNCF_Secuencia: TBCDField
      FieldName = 'NCF_Secuencia'
      Precision = 18
      Size = 0
    end
    object QTicketitbis: TBCDField
      FieldName = 'itbis'
      currency = True
      Precision = 15
      Size = 2
    end
    object QTicketnombre: TStringField
      FieldName = 'nombre'
      Size = 80
    end
    object QTicketrnc: TStringField
      FieldName = 'rnc'
      Size = 30
    end
    object QTicketstatus: TStringField
      FieldName = 'status'
      Size = 3
    end
    object QTicketNCF: TStringField
      FieldKind = fkCalculated
      FieldName = 'NCF'
      Calculated = True
    end
  end
  object dsTicket: TDataSource
    DataSet = QTicket
    OnDataChange = dsTicketDataChange
    Left = 192
    Top = 200
