    SQL.Strings = (
      'select'
      'ticket, fecha_hora, fecha, total, itbis, usu_codigo, caja'
      'from'
      'montos_ticket'
      'where'
      'usu_codigo = :usu'
      'and caja = :caj'
      'and fecha = convert(datetime, :fec, 105)'
      'and status = '#39'TMP'#39
      'order by'
      'fecha_hora desc')
