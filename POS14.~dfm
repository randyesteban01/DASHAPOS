  object QClientes: TADOQuery
    Connection = DM.ADOSIGMA
    Parameters = <>
    SQL.Strings = (
      'select'
      
        'distinct emp_codigo, cli_codigo, cli_nombre, cli_rnc, cli_cedula' +
        ', '
      'isnull(cli_limite,0) as cli_limite, cli_balance, emp_numero,'
      'cli_direccion, cli_localidad, tfa_codigo, cli_facturarbce,'
      'cli_facturarvencida, Aumento'
      'from'
      'clientes'
      'order by'
      'cli_nombre, emp_codigo')
    Left = 192
    Top = 160
