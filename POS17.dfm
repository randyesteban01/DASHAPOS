      item
        Name = 'fecha2'
        Attributes = [paNullable]
        DataType = ftDateTime
        NumericScale = 3
        Precision = 23
        Size = 16
        Value = Null
      end>
    SQL.Strings = (
      'select'
      'm.caja, m.usu_codigo, m.ticket, m.total,'
      'm.ncf_fijo, m.ncf_secuencia, m.itbis,'
      '(m.itbis * 100)/16 as grabado,'
      'm.total - ((m.itbis * 100)/16) as excento,'
      'm.nombre, m.status, m.fecha_hora, u.usu_nombre'
      'from'
      'montos_ticket m, usuarios u'
      'where'
      'm.usu_codigo = u.usu_codigo'
      'and m.fecha_hora between convert(datetime, :fecha1, 113)'
      'and convert(datetime, :fecha2, 113)')
