  object QProductos: TADOQuery
    Connection = DM.ADOSIGMA
    Parameters = <
      item
        Name = 'fecha1'
        Attributes = [paNullable]
        DataType = ftDateTime
        NumericScale = 3
        Precision = 23
        Size = 16
        Value = Null
      end
      item
        Name = 'fecha2'
        Attributes = [paNullable]
        DataType = ftDateTime
        NumericScale = 3
        Precision = 23
        Size = 16
        Value = Null
      end>
    SQL.Strings = (
      'select'
      't.descripcion, sum(t.cantidad) as cantidad,'
      'sum(t.cantidad*t.precio) as monto'
      'from'
      'montos_ticket m, ticket t'
      'where'
      'm.fecha = t.fecha'
      'and m.usu_codigo = t.usu_codigo'
      'and m.caja = t.caja'
      'and m.ticket = t.ticket'
      'and m.fecha_hora between convert(datetime, :fecha1, 113)'
      'and convert(datetime, :fecha2, 113)'
      'group by'
      't.descripcion')
    Left = 632
    Top = 312
    object QProductosdescripcion: TStringField
      FieldName = 'descripcion'
      Size = 80
    end
    object QProductoscantidad: TBCDField
      FieldName = 'cantidad'
      ReadOnly = True
      currency = True
      Precision = 32
      Size = 2
    end
    object QProductosmonto: TBCDField
      FieldName = 'monto'
      ReadOnly = True
      currency = True
      Precision = 32
    end
  end
